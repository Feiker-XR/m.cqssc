{
	"compiles":{
		"css":{
			"common":{
				"from":"public/src/sass/*.scss",
				"mid":"public/mid/css",
				"to":"public/dist/css"
			},
			"login":{
				"from":"public/src/sass/login/*.scss",
				"mid":"public/mid/css",
				"to":"public/dist/css"
			}
		
		},
		"scripts":{
			"common":{
				"from":"public/src/scripts/common/*.js",
				"mid":"public/mid/scripts",
				"to":"public/dist/scripts"
			},
			"login":{
				"from":"public/src/scripts/login/*.js",
				"mid":"public/mid/scripts",
				"to":"public/dist/scripts"
			}
			
		},
		"images":{
			"from":"public/src/imgs/**/*.{jpeg,jpg,png,gif}",
			"mid":"public/mid/imgs",
			"to":"public/dist/imgs"
		}
	}
}
